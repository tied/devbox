#! /usr/bin/env bash

source selfedit.sh

# @COMMAND up [profile]                  spin up a new db with profile
function up() {
    _with_arguments 1 $@
    profile=${1}
    docker-compose -f ${VIQUEEN_DEVBOX_HOME}/docker-compose/${profile}.yml up
}

function down() {
    _with_arguments 1 $@
    profile=${1}
    docker-compose -f ${VIQUEEN_DEVBOX_HOME}/docker-compose/${profile}.yml down
}

eval $@
