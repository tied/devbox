#! /usr/bin/env bash

_product_name() {
    echo "crowd"
}

_product_definition() {
    _with_arguments 1 $@
    version=${1}
    echo "--product crowd
          --version ${version}
          --http-port ${HTTP_PORT:-4990}
          --ajp-port ${AJP_PORT:-4009}
          --server 127.0.0.1"
}

source atlassian-product.sh

eval $@

if [[ -z $1 ]]; then
    $0 -h
fi